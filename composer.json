{
    "name": "lewisjenkins/craft-default-text",
    "description": "Adds a Default Value setting to the Plain Text field.",
    "type": "craft-plugin",
    "version": "1.0.7",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "craft default text",
        "lj default text"
    ],
    "support": {
        "docs": "https://github.com/lewisjenkins/craft-default-text/blob/master/README.md",
        "issues": "https://github.com/lewisjenkins/craft-default-text/issues",
        "email": "lewis@lj.io"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Lewis Jenkins",
            "homepage": "https://lj.io"
        }
    ],
    "require": {
        "craftcms/cms": "^3.6.0"
    },
    "autoload": {
        "psr-4": {
          "lewisjenkins\\craftdefaulttext\\": "src/"
        }
    },
    "extra": {
        "name": "LJ Default Text",
        "handle": "craft-default-text",
        "developer": "Lewis Jenkins",
        "developerUrl": "https://lj.io",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/lewisjenkins/craft-default-text/master/CHANGELOG.md",
        "documentationUrl": "https://github.com/lewisjenkins/craft-default-text/blob/master/README.md",
        "class": "lewisjenkins\\craftdefaulttext\\CraftDefaultText"
    }
}
